{
    // storage system will be initialized with this name
    "storage": "zk",
    // server configurations
    "server":{
        "port":20001
    },
    // log configurations
    "log":{
        "appenders": {
          "console":{"type": "console"}
        },
        "categories": { "default": { "appenders": ["console"], "level": "debug" } }
    },
    // system paths
    "path":{
        "controller":"./src/controller",
        "router":"./src/router"
    },
    "end":""
}